    {
      "id": 16834597,
      "name": "Iroha",
      "events": [
        {
          "id": 2,
          "dialogue": [
            {
              "id": 7300,
              "string": "1\u0000\u0007Master, what is this place?"
            },
            {
              "id": 7352,
              "string": "1\u0000\u0007The ambassador...that mask..."
            }
          ]
        },
        {
          "id": 3,
          "dialogue": [
            
          ]
        },
        {
          "id": 4,
          "dialogue": [
            {
              "id": 7375,
              "string": "1\u0000\u0007Does he truly mean to call forth an endless void...?"
            },
            {
              "id": 7382,
              "string": "1\u0000\u0007The ambassador...I must stop him.1\u0000\u0007\u0000"
            }
          ]
        },
